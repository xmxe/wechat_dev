<scroll-view scroll-y class="scrollPage">
<cu-custom bgColor="bg-gradual-green" >
	<view slot="content">医院列表</view>
</cu-custom>

<view class="cu-card">
    <view class="cu-item bg-img shadow-blur" style="background-image:url(https://image.weilanwl.com/color2.0/plugin/sylb2244.jpg)" bindtap="toHospitalDeatail" data-id="{{item.id}}" wx:for="{{hospitalList}}" wx:key>
      <view class="cardTitle">
        {{item.name}}
      </view>
    </view>
  </view>
  <view class="cu-tabbar-height"></view>

</scroll-view>